    <!-- config_value 定義的是撥放序列，對應到底下列表的音檔，舉例來說 config_value 裡頭的 "3,3,3,3,3,3,3,..." 會連續撥放七次在列表中的第三個音檔("audio/#3.mp3")-->
    <!-- config_value 撥放序列跟 config_audio & config_name 裡頭的數字或名字沒關係，單純只對照 audio tag 宣告的順序-->
    <model livelauncher:config_name="sounds" livelauncher:config_value="3,3,3,3,3,3,3,5,1,2,3,4,4,4,4,4,3,3,3,2,2,1,2,5,3,3,3,3,3,3,3,5,1,2,3,4,4,4,4,4,3,3,5,5,4,2,1">
        <audio
            livelauncher:config_audio="audio/#1.mp3"
            livelauncher:config_name="#1" />
        <audio
            livelauncher:config_audio="audio/#2.mp3"
            livelauncher:config_name="#2" />
        <audio
            livelauncher:config_audio="audio/#3.mp3"
            livelauncher:config_name="#3" />
        <audio
            livelauncher:config_audio="audio/#4.mp3"
            livelauncher:config_name="#4" />
        <audio
            livelauncher:config_audio="audio/#5.mp3"
            livelauncher:config_name="#5" />
    </model>
